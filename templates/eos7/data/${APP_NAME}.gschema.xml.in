<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="@APP_PATH@" id="@APP_ID@">
    <key name="window-height" type="i">
      <default>475</default>
      <summary>Most recent window height</summary>
      <description>Most recent window height</description>
    </key>
    <key name="window-width" type="i">
      <default>650</default>
      <summary>Most recent window width</summary>
      <description>Most recent window width</description>
    </key>
    <key name="window-maximized" type="b">
      <default>false</default>
      <summary>Open window maximized.</summary>
      <description>Whether the main window of the application should open maximized or not.</description>
    </key>
    
    <key name="pane-visible" type="b">
      <default>true</default>
      <summary>Pane visibility when at tablet viewport</summary>
      <description>Whether pane should be visible at tablet viewport</description>
    </key>
    <key name="pane-position" type="i">
      <default>370</default>
      <summary>Pane position between the queue and now playing view</summary>
      <description>Pane position between the queue and now playing view</description>
    </key>
  </schema>
</schemalist>